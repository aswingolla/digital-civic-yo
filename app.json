{
  "expo": {
    "name": "civic-yo",
    "slug": "civic-yo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/civic_yo_icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/civic_yo_icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/civic_yo_icon.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/civic_yo_icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "output": "server",
      "favicon": "./assets/civic_yo_icon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}