{
  "expo": {
    "name": "minimal-expo-router",
    "slug": "minimal-expo-router",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/civic_yo_icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/civic_yo_icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/civic_yo_icon.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/civic_yo_icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/civic_yo_icon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "/",
          "webRouting": "hash"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}