{
  "expo": {
    "name": "civic-yo",
    "slug": "civic-yo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/civic_yo_icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/civic_yo_icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.civic.yo",
      "supportsTablet": true,
      "icon": "./assets/civic_yo_icon.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/civic_yo_icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.civic.yo"
    },
    "web": {
      "output": "server",
      "bundler": "metro",
      "favicon": "./assets/civic_yo_icon.png"
    },
    "plugins": [
      "expo-router",
      "@logrocket/react-native",
      "expo-build-properties"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "90d54407-9750-455e-b89e-c4fbf1fc637b"
      }
    },
    "owner": "aswgolla"
  }
}
